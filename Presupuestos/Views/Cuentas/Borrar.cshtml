@model Presupuestos.Models.Cuenta

@{
    ViewData["Title"] = "Borrar Cuenta";
}

<h1>Borrar</h1>

<h3>seguro que desea borrar la cuenta @Model.Nombre.?</h3>
<div>
  
    <form asp-action="Borrar">
        <input type="hidden" asp-route-id="Id" />
        <button type="submit" class="btn btn-danger">Borrar</button>        
        <a  class="btn btn-secondary" asp-action="Index">Cancelar</a>
    </form>
</div>
